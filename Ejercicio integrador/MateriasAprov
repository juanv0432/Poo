class MateriasAprobadas:
    __Dni=''
    __NomMateria=''
    __fecha=''
    __nota=0.0
    __aprobacion=''
    def __init__(self,DNI,nomMat,fecha,nota,aprobacion):
        self.__Dni=DNI
        self.__NomMateria=nomMat
        self.__fecha=fecha
        self.__nota=nota
        self.__aprobacion=aprobacion
    def getDni(self):
        return self.__Dni
    def getNota(self):
        return float(self.__nota)
    def getNombre(self):
        return self.__NomMateria
    def getFecha(self):
        return self.__fecha
    def getAprov(self):
        return self.__aprobacion